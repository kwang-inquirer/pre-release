<?xml version="1.0" encoding="utf-8"?><Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <outboundMessages>
        <fullName>Lead_Phone_DNC_update_via_Informatica</fullName>
        <apiVersion>46.0</apiVersion>
        <endpointUrl>https://na1.dm-us.informaticacloud.com/saas/api/1/salesforceoutboundmessage/GFND1qEfILM37XHqKLFWF4KRgQiIGbeE</endpointUrl>
        <fields>Id</fields>
        <fields>Phone</fields>
        <includeSessionId>false</includeSessionId>
        <integrationUser>infrmtc_intgrtn-acct@inquirer.com</integrationUser>
        <name>Lead Phone DNC update via Informatica</name>
        <protected>false</protected>
        <useDeadLetterQueue>false</useDeadLetterQueue>
    </outboundMessages>
    <rules>
        <fullName>Lead Phone DNC update  using OBM to Informatica</fullName>
        <actions>
            <name>Lead_Phone_DNC_update_via_Informatica</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <formula>AND(   ISCHANGED(Phone),   LEN(Phone) &gt; 0,   NOT(     CONTAINS(       $Profile.Name &amp; $User.FirstName &amp; $User.LastName,       'Integration'     )   ) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
