    <rules>
        <fullName>Contact Phone DNC update  using OBM to Informatica</fullName>
        <actions>
            <name>Contact_Phone_DNC_update_via_Informatica</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <formula>AND(   ISCHANGED(Phone),   LEN(Phone) &gt; 0,   NOT(     CONTAINS(       $Profile.Name &amp; $User.FirstName &amp; $User.LastName,       'Integration'     )   ) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Modify Contact</fullName>
        <actions>
            <name>Modify_Contact</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <formula>AND(   LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com',  NOT(ISBLANK( DSI_Person_Id__c )),  OR(ISCHANGED( FirstName ), ISCHANGED( LastName ), ISCHANGED(MiddleName))  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Send Contact</fullName>
        <actions>
            <name>Contact_OBM</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <formula>AND(CreatedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com',  ISBLANK( DSI_Person_Id__c ))</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Send OBM</fullName>
        <active>true</active>
        <formula>true</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
