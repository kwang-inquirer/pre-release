    <rules>
        <fullName>Create DH Tag</fullName>
        <actions>
            <name>Create_DH_Tag</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND(ISCHANGED(Premium_Opt_Out__c),Premium_Opt_Out__c=True, LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com')</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Delete DH Tag</fullName>
        <actions>
            <name>Delete_DH_Tag</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND(ISCHANGED(Premium_Opt_Out__c),Premium_Opt_Out__c=False, LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com')</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>EZPaySignUp</fullName>
        <actions>
            <name>EZPay</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Subscription__c.subscription_Auto_Renew__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Send EZPay Email Alert when EZPay Sign up happens</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Send Attention Line Update</fullName>
        <actions>
            <name>Send_Attention_Line</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND(ISCHANGED( Attention_Line__c), LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com')</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Send Split Delivery</fullName>
        <actions>
            <name>Send_Split_Delivery</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND(ISChanged( Ready_for_DSI__c ), LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com', Ready_for_DSI__c = true,  DSI_Subscription_ID__c != null)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Send Subscription</fullName>
        <actions>
            <name>Send_Subscription</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND(ISChanged( Ready_for_DSI__c ), LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com', Ready_for_DSI__c = true,  DSI_Subscription_ID__c = null)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>UpdateHouseholdContactTextField</fullName>
        <actions>
            <name>UpdateContactTextField</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>UpdateHouseholdTextField</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>This workflow will fire and update a Household and Contact text field with the name from the lookup fields for those on a Subscription only when either of those lookups are changes</description>
        <formula>OR(ISNEW(), (ISCHANGED(subscription_Account__c)), (ISCHANGED( Contact__c )) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
