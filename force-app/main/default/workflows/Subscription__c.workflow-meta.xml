    <fieldUpdates>
        <fullName>UpdateContactTextField</fullName>
        <description>This field update will update the ContactTextSearch on Subscription to equal the name of the contact</description>
        <field>ContactTextSearch__c</field>
        <formula>Contact__r.FirstName&amp;" "&amp;Contact__r.MiddleName&amp;" " &amp;Contact__r.LastName</formula>
        <name>UpdateContactTextField</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
