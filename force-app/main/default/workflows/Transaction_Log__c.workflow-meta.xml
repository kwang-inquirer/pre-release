    <rules>
        <fullName>Clear Restart Date</fullName>
        <actions>
            <name>Clear_Restart_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Clear_Restart_Memo</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Clear_Restart_Source</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( Clear_Restart_Date__c = TRUE, ISCHANGED( Clear_Restart_Date__c ))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Delete Complaint</fullName>
        <actions>
            <name>Delete_Complaint</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND(ISCHANGED(Cancelled__c), Cancelled__c = TRUE, ISPICKVAL(Transaction_Type__r.Category__c,'Complaint'), LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com')</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Modify Start</fullName>
        <actions>
            <name>Modify_Start</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND( LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com',   OR( Complaint_Code__c = 'TVR', Complaint_Code__c = 'TVW'), ISCHANGED( First_Date__c ))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Modify Stop</fullName>
        <actions>
            <name>Send_Modify_Stop</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND( LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com',   OR( Complaint_Code__c = 'TVR', Complaint_Code__c = 'TVW', ISPICKVAL(Transaction_Type__r.Category__c, 'Stop')), ISCHANGED( Last_Date__c ))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Modify Vacation Hold</fullName>
        <actions>
            <name>Modify_Vacation_Hold</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND( LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com',  Complaint_Code__c = 'VH', OR(ISCHANGED( First_Date__c ), ISCHANGED( Last_Date__c )))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Modify Vacation Package</fullName>
        <actions>
            <name>Modify_Vacation_Package</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND( LastModifiedBy.Username != 'infrmtc_intgrtn-acct@inquirer.com',  Complaint_Code__c = 'VPA', OR(ISCHANGED( Last_Date__c ), ISCHANGED( First_Date__c )))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Send Carrier Memo</fullName>
        <actions>
            <name>Send_Carrier_Memo</name>
