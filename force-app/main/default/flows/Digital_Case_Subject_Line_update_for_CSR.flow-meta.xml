    <description>This process builder to fire when a digital case&apos;s Category or Type is changed.  It updates the Subject to be Category + Type for a CSR</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>TEXT([Case].Type )+&quot; &quot;+ &quot;+ &quot;+ TEXT([Case].Category__c)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_9354748528</name>
        <dataType>String</dataType>
        <expression>TEXT({!myVariable_current.Type} )+&quot; &quot;+ &quot;+ &quot;+ TEXT({!myVariable_current.Category__c})</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
[Case].Owner:User.Profile.Name != &quot;Subscriber Community&quot;,
[Case].RecordType.DeveloperName =&quot;Digital_Case&quot;,
OR(
ISNEW(),
OR(
ISCHANGED([Case].Category__c ),
ISCHANGED([Case].Type)
)
)
)
</stringValue>
